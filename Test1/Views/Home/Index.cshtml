@{
    ViewData["Title"] = "Index";
}

<br />
<div class="container row p-0 m-0">
    <div class="col-9">
        <h2 class="text-info">Таблица</h2>
    </div>
    <div class="col-3">
        <button type="submit" class="btn btn-primary form-control">Загрузить в таблицу</button>
    </div>
    <div class="col-12 border p-3 mg-3">
        <form method="post">
            <table id="CV_table"class="table table-striped table-bordered" style="width:100%">
                <thread>
                    <tr>
                        <th>index</th>
                        <th>code</th>
                        <th>value</th>
                    </tr>
                </thread>
            </table>
        </form>
    </div>
</div>

@section Scripts{ 
    <script src="~/js/CodeValueList.js"></script>
}